<li class="list-group">

    <div class="list-group-item">
        <h4><a href="#" class="title">{{title}}</a></h4>
        <span class="field"><b>{{date_start}}</b></span>  
        {{#if date_end}} - <span class="date">{{date_end}}</span> {{/if}}
        {{#if country}} - <span class="country">{{country}}</span> {{/if}}
        {{#if venue}} - <span class="venue">{{venue}}</span> {{/if}}
		<br />
		  {{#if category}}

		        {{#each category}}
		    		<span class="label label-default">{{this}}</span> 
				{{/each}}
		        
		        {{/if}}
    </div>
    <div class="list-group-item">
        <p class="intro">
		 {{description}}
		
		  {{#if long_description}}
		 <br />
		<a data-toggle="collapse" data-parent="#accordion" href="#readmore{{id}}" aria-expanded="true" aria-controls="collapseOne">
	    	<span class="caret"></span> <small>Read More</small>
	    </a>
		<div id="readmore{{id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
	  <div class="panel-body">
	   {{long_description}}
		</div>
		</div>
		  {{/if}}
        </p>
    </div>

	<div class="panel panel-default">
	
	 {{#if attachments.PI}}
	 <div class="panel-heading" role="tab" id="headingOne">
	 <h4 class="panel-title">
	    <a data-toggle="collapse" data-parent="#accordion" href="#pict{{id}}" aria-expanded="true" aria-controls="collapseOne">
	    	<span class="caret"></span> <small>Pictures</small>
	    </a>
	  </h4>
	  </div>
		<div id="pict{{id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
 
			<div class="list-group-item item-line">
		        <span class="afo-field-title field">Pictures</span><ul class="afo-category-list">
		       {{#each attachments.PI}}
			   <li><img src="{{this.file_name}}" target="blank" onerror="this.src='images/nopic.jpg'"/>
			   {{this.attachment_title}}
			   </li>	
			   {{/each}}
		       </ul>
		    </div>
			
		</div>
	 {{/if}}

	<div class="panel-heading" role="tab" id="headingOne">
	  <h4 class="panel-title">
	    <a data-toggle="collapse" data-parent="#accordion" href="#collapse{{id}}" aria-expanded="true" aria-controls="collapseOne">
	    	<span class="caret"></span> <small>Attachments</small>
	    </a>
	  </h4>
	</div>
	<div id="collapse{{id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
	  <div class="panel-body">

		   
			 {{#if attachments.L}}
			<div class="list-group-item item-line">
		        <span class="afo-field-title field">Links</span>
				<ul class="afo-category-list">
		       {{#each attachments.L}}
		    					<li><a href="{{this.file_name}}" target="blank">{{this.attachment_title}}</a>
						 {{#if this.attachment_description}}<ul>{{this.attachment_description}}</ul>{{/if}}
						</li>	
				{{/each}}
		       </ul>
		       
		    </div>
			 {{/if}}
			 
			  {{#if attachments.PR}}
			<div class="list-group-item item-line">
		        <span class="afo-field-title field">Presentations</span><ul class="afo-category-list">
		       {{#each attachments.PR}}
		    				    			<li><a href="{{this.file_name}}" target="blank">{{this.attachment_title}}</a>
											 {{#if attachment_size}}({{attachment_size}}) {{/if}}
						 {{#if this.attachment_description}}<ul>{{this.attachment_description}}</ul>{{/if}}
						</li>		{{/each}}
		       </ul>
		    </div>
			 {{/if}}
			 
			 
			  {{#if attachments.D}}
			<div class="list-group-item item-line">
		        <span class="afo-field-title field">Documents</span><ul class="afo-category-list">
		       {{#each attachments.D}}
		    			    			<li><a href="{{this.file_name}}" target="blank">{{this.attachment_title}}</a>
										 {{#if attachment_size}}({{attachment_size}}) {{/if}}
						 {{#if this.attachment_description}}<ul>{{this.attachment_description}}</ul>{{/if}}
						</li>	 {{/each}}
		       </ul>
		    </div>
			 {{/if}}
			 
			
			 


	  </div>
	
	</div>
	</div>
</li>